<% provide(:title, "Create Post") %>
<h1>Create Post</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:post) do |f| %>

      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>

      <%= f.label :content %>
      <%= f.text_field :content, class: 'form-control' %>
      <%= f.submit "Creat", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
